{
    "name": "@zenflux/utils",
    "version": "0.1.0",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src",
        "package.json",
        "tsconfig.json",
        "tsconfig.eslint.json"
    ],
    "exports": {
        "./package.json": "./package.json",
        "./src/*": [
            "./src/*.js",
            "./src/*.ts"
        ]
    },
    "scripts": {
        "build-types": "rm src/*.d.ts >/dev/null 2>&1 ; tsc -p ./tsconfig.json"
    }
}
