{
  "name": "@zenflux/cli",
  "version": "0.1.30",
  "type": "module",
  "types": "./types/index.d.ts",
  "bin": {
    "@z-cli": "bin/cli.js"
  },
  "scripts": {
    "z-cli:build-types": "tsc -p tsconfig.types.json ; node types/replace-imports.js types \"@zenflux/cli\" \"./\" ; rm -rf dist"
  },
  "files": [
    "bin",
    "scripts",
    "src",
    "types",
    "tsconfig.json",
    "tsconfig.eslint.json",
    "tsconfig.paths.json",
    "tsconfig.types.json"
  ],
  "dependencies": {
    "@microsoft/api-extractor": "^7.53.2",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@swc/core": "latest",
    "@verdaccio/config": "next",
    "@verdaccio/node-api": "next",
    "@zenflux/tsconfig-to-swc": "workspace:*",
    "@zenflux/typescript-vm": "workspace:*",
    "@zenflux/utils": "workspace:*",
    "@zenflux/worker": "workspace:*",
    "acorn": "^8.15.0",
    "blessed": "^0.1.81",
    "chokidar": "^3.5.3",
    "libnpmpack": "^9.0.9",
    "libnpmpublish": "^11.1.2",
    "magic-string": "^0.30.5",
    "node-fetch": "^3.3.2",
    "npm-packlist": "^10.0.3",
    "pacote": "^21.0.3",
    "rollup": "^4.9.1",
    "semver": "^7.6.3",
    "tslib": "^2.6.2",
    "typescript": "latest",
    "verdaccio": "^5.29.2"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/bun": "^1.1.3",
    "@types/libnpmpublish": "^4.0.5",
    "@types/node-fetch": "^2.6.13",
    "@types/npm-packlist": "^7.0.2",
    "@types/pacote": "^11.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json",
    "./src/*": [
      "./src/*.ts"
    ],
    "./src/modules/console": [
      "./src/modules/console/index.ts"
    ]
  }
}